for (let i = 1; i < 501; i++) {
  if (501 % i === 0) console.log("FIZZBUZZ++");
  else if (i % 15 === 0) console.log("FIZZBUZZ");
  else if (i % 3 === 0) console.log("FIZZ");
  else if (i % 5 === 0) console.log("BUZZ");
  else console.log(i);
}

